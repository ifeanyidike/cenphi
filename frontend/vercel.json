{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "builds": [{ "src": "package.json", "use": "@vercel/node" }],
  "routes": [
    {
      "src": "/testimonial-widget.umd.js",
      "dest": "/dist/testimonial-widget.umd.js"
    },
    {
      "src": "/testimonial-widget.css",
      "dest": "/dist/testimonial-widget.css"
    },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
